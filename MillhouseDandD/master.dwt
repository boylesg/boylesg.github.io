		<script type="text/javascript">
		
			function DoSetBGImage()
			{
				let nImgNum = Math.trunc(Math.random() * 6) + 1,
					strImgName = "D&D" + nImgNum.toString() + ".jpg";
					
				document.body.style.backgroundImage = "url('images/" + strImgName + "')";
			}
			
			function DoPageLoadStuff()
			{
				var cell = null;
				
				if (document.title == "Learn")
				{
					cell = document.getElementById("learn");
				}
				else if (document.title == "Rules")
				{
					cell = document.getElementById("rules");
				}
				else if (document.title == "Calendar")
				{
					cell = document.getElementById("calendar");
				}
				else if (document.title == "Contact")
				{
					cell = document.getElementById("contact");
				}
				else if (document.title == "Collections")
				{
					cell = document.getElementById("collections");
				}
				if (cell)
				{
					cell.style.borderStyle = "solid";
					cell.style.borderWidth = "thin";
					cell.style.borderColor = "#B90000";
				}
			}
			
			function DoToggle(strIDElementToHide, strIDUpImage, strIDDownImage)
			{
				let div = document.getElementById(strIDElementToHide),
					imgUp = document.getElementById(strIDUpImage),
					imgDown = document.getElementById(strIDDownImage);
				
				if (div && imgUp && imgDown)
				{
					if ((div.style.height == "auto") || (div.style.height == ""))
					{
						div.style.height = "30px";
						div.style.overflow = "hidden";
						imgDown.style.display = "inline";
						imgUp.style.display = "none";
					}
					else
					{
						div.style.height = "auto";
						div.style.overflow = "auto";
						imgDown.style.display = "none";
						imgUp.style.display = "inline";
					}
				}
			}
			
		</script>
		
